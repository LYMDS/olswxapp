#gar_box{
  background-color: #e2fff5;
  width: 100%;
  height: 1106rpx;
  overflow: auto;
  position: relative;
  z-index: 1;
}

.side_box{
  display: inline-flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
  position: relative;
  height: 180rpx;
  width: 180rpx;
  margin-left: 50rpx;
  font-size: 20rpx;
}
.side_box:hover{
  background-color: aquamarine;
}

.row_box{
  display: flex;
  flex-flow: row nowrap;
}

.garage_frame{
  position: absolute;
  top: 0%;
  left: 0%;
  height: 100%;
  width: 100%;
}
.car{
  position: absolute;
  bottom: 3%;
  height: 30%;
  width: 60%;
}
.car_num{
  position: absolute;
  top: 45%;
  background-color: rgb(0, 0, 143);
  color: white;
}
.battery{
  position: absolute;
  left: 0%;
  bottom: 8%;
  width: 15%;
  height: 25%;
}
.car_side{
  color: white;
  font-size: 30rpx;
  position: absolute;
  top: 17%;
}


.solid {
  border-bottom-width: 10px;
  border-bottom-color: grey;
  border-bottom-style: solid;
  height: 160rpx;
  width: 180rpx;
  display: flex;
  justify-content: center;
}

.dashed {
  border-left-style: dashed;
  border-left-color: grey;
  border-left-width: 1px;
  width: 140rpx;
  height: 180rpx;
  border-right-style: dashed;
  border-right-color: grey;
  border-right-width: 1px;
}

.bottombox{
  width: 100%;
  height: 100rpx;
  position: absolute;
  bottom: 0rpx;
  background-color: aquamarine;
  display: flex;
  justify-content: center;
  align-items: center;
  color: gray;
  z-index: 0;
}
.bottombox:active{
  background-color: rgb(97, 192, 160);
}

page{
  background-color: #e2fff5;
}