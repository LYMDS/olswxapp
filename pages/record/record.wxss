.reduce {
  display: flex;
  flex-direction: column;
  line-height: 30rpx;
  border: 1px solid rgb(245, 242, 242);
  margin-top: 30rpx;
  width: 90%;
}

.information {
  display: flex;
  flex-direction: row;
  font-size: 50rpx;
  height: 150rpx;
  align-items: center;
  background-color: rgb(233, 230, 230);
  width: 100%;
}

.timecontent{
  margin-left: 50rpx;
}

.reducecontent{
  display: flex;
}



.reduce-detail{
  height: 400rpx
}

.charge {
  display: flex;
  flex-direction: column;
  margin-top: 30rpx;
  line-height: 30rpx;
  border: 1px solid rgb(233, 230, 230);
  width: 90%;
}

.charge-detail{
  height: 210rpx
}

.charge-information {
  display: flex;
  flex-direction: row;
  font-size: 50rpx;
  color: green;
  height: 150rpx;
  align-items: center;
  background-color: rgb(233, 230, 230);
  width: 100%;
}

.record{
  display: flex;
  flex-direction: column;
