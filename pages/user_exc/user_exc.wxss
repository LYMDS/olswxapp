@import "../../iconfont.wxss";

@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1165653_lpq5zdduwmn.eot?t=1560756463148'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1165653_lpq5zdduwmn.eot?t=1560756463148#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1165653_lpq5zdduwmn.woff?t=1560756463148') format('woff'),
  url('//at.alicdn.com/t/font_1165653_lpq5zdduwmn.ttf?t=1560756463148') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1165653_lpq5zdduwmn.svg?t=1560756463148#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 80rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-xiangji:before {
  content: "\e723";
}

.icon-camera:before {
  content: "\e619";
}

.icon-che:before {
  content: "\e601";
}



page{
  display: flex;
  flex-direction: column;
}

.type{
  height : 150rpx;
  width: 150rpx;
  border: solid 1px rgba(0, 0, 0 , 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  font-size: 30rpx;
  background-color: rgb(174, 212, 212);
}
.type:hover{
  background-color: rgb(87, 139, 87);
}


#garage_num{
  width: 300rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}


#input_button{
  display: flex;
  background-color: rgba(0, 0, 0, 0.1);
  height: 100rpx;
  width: 90%;
  margin-left: 5%;
  font-size: 35rpx;
  border-radius: 50rpx;
  color: rgb(0, 0, 0);
  justify-content: flex-start;
  align-items: center;
  margin-top: 20rpx;
}


#guide{
  width: 235rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 25rpx;
}


#scancode{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 50rpx;
}


.tip{
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  margin-left: 5%;
  font-size: 25rpx;
  color: rgba(0, 0, 0, 0.400)
}

#type_box{
  display: flex;
  flex-flow: nowrap row;
  justify-content: space-around;
  margin-top: 30rpx;
}

#except_text{
  background-color: rgba(0, 0, 0, 0.1);
  margin-left: 5%;
  width: 82%;
  height: 200rpx;
  margin-top: 30rpx;
  padding: 4% 4% 4% 4%;
  border-radius: 60rpx;
  font-size: 32rpx;
}

#choose_photo{
  height:150rpx;
  width: 150rpx;
  
  margin-left: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
}

#button_box{
  width: 90%;
  margin-left: 5%;
  margin-top: 50rpx;
  background-color: green;
  color: #fff
}

.readypost{
  height:100rpx;
  width:100rpx;
}

#img_box{
  margin-left: 5%;
  display: flex;
  height: 100rpx;
  align-items: center;
  flex-flow: row nowrap;
}